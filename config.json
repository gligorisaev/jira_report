{
  "dashboard": {
    "title": "Jira Xray Test Coverage Dashboard",
    "theme": {
      "primaryColor": "#667eea",
      "secondaryColor": "#764ba2",
      "successColor": "#48bb78",
      "dangerColor": "#fc8181",
      "warningColor": "#f6ad55",
      "neutralColor": "#a0aec0"
    },
    "charts": {
      "coverage": {
        "enabled": true,
        "type": "doughnut"
      },
      "testStatus": {
        "enabled": true,
        "type": "bar"
      }
    },
    "features": {
      "expandableEpics": true,
      "showTestDetails": true,
      "progressBars": true,
      "statusBadges": true
    }
  },
  "csv": {
    "delimiter": ";",
    "encoding": "utf-8",
    "columns": {
      "parentKey": "Parent Requirement Key",
      "parentSummary": "Parent Requirement Summary",
      "requirementKey": "Requirement Key",
      "requirementSummary": "Requirement Summary",
      "requirementStatus": "Requirement Status",
      "testKey": "Test Key",
      "testSummary": "Test Summary",
      "testStatus": "Test Status",
      "defectKeys": "Defect Keys"
    }
  },
  "output": {
    "defaultFilename": "dashboard.html",
    "includeTimestamp": true,
    "openBrowser": false
  }
}
